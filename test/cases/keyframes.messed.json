{
  "stylesheet": {
    "rules": [
      {
        "name": "fade",
        "keyframes": [
          {
            "selectors": [
              "from"
            ],
            "declarations": [
              {
                "property": "opacity",
                "value": "0"
              }
            ]
          },
          {
            "selectors": [
              "to"
            ],
            "declarations": [
              {
                "property": "opacity",
                "value": "1"
              }
            ]
          }
        ]
      }
    ]
  }
}
